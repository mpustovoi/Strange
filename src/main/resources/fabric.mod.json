{
  "schemaVersion": 1,
  "id": "strange",
  "version": "${mod_version}",
  "name": "Strange",
  "description": "Charm, but stranger.",
  "authors": [
    "svenhjol",
    "Coranthes"
  ],
  "icon": "icon.png",
  "license": "MIT",
  "environment": "*",
  "mixins": [
    "strange.mixins.json"
  ],
  "entrypoints": {
    "main": [
      "svenhjol.strange.fabric.Initializer"
    ],
    "client": [
      "svenhjol.strange.fabric.ClientInitalizer"
    ],
    "modmenu": [
      "svenhjol.strange.integration.modmenu.StrangeModMenuPlugin"
    ]
  },
  "accessWidener": "strange.accesswidener",
  "depends": {
    "fabricloader": ">=${loader_version}",
    "charmony": ">=${charmony_version}",
    "minecraft": "${minecraft_version}",
    "java": ">=17"
  },
  "contact": {
    "homepage": "https://svenhjol.github.io/Strange",
    "sources": "https://github.com/svenhjol/Strange"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "${discord_link}"
      },
      "update_checker": true
    }
  }
}